{
    "name": "zhan3333/api-tester",
    "description": "Api tester for laravel and dingo api",
    "type": "library",
    "keywords": ["laravel-admin", "api", "tester", "dingo"],
    "homepage": "https://github.com/laravel-admin-extensions/api-tester",
    "license": "MIT",
    "authors": [
        {
            "name": "z-song",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "encore/laravel-admin": "1.5.*",
        "dingo/api": "1.0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\ApiTester\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Admin\\ApiTester\\ApiTesterServiceProvider"
            ]
        }
    }
}
